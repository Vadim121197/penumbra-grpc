{
    "manifest_version": 3,
    "name": "React Extension",
    "description": "Chrome Extension in React!",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html",
        "default_title": "React Extension",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts":[
        {
             "matches": ["http://*/*", "https://*/*"],
            "js": ["contentScript.js"]
        }
    ],
    "permissions": [
            "storage", "tabCapture", "alarms", "notifications"
    ]
}